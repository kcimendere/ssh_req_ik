								</m:Column>
							</m:columns>
							<m:items>
								<m:ColumnListItem>
									<m:cells>
										<m:Link text="{detail>Filename}" target="_blank" href="{detail>UrlToDisplay}"/>
										<m:Text text="{detail>ContentType}"/>
										<m:Text text="{detail>CreatedBy}"/>
